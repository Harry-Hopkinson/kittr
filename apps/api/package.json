{
	"name": "api",
	"version": "0.0.0",
	"scripts": {
		"compile": "tsc && node ./dist/src/index.js",
		"dev": "nodemon -e ts",
		"build": "rm -rf dist && tsc",
		"start": "node ./dist/src/index.js",
		"test": " jest --coverage",
		"test:watch": "jest --watch --coverage",
		"lint": "eslint \"**/*.ts\" --fix --ignore-pattern node_modules/",
		"clean": "rm -rf .turbo && rm -rf coverage && rm -rf dist && rm -rf node_modules",
		"type-check": "tsc --pretty --noEmit",
		"ci:test": "cross-env DATABASE_URL=test jest .*\\.unit\\.test\\.ts"
	},
	"dependencies": {
		"@google-analytics/data": "2.8.0",
		"@kittr/config": "file:packages/config",
		"@kittr/logger": "file:packages/logger",
		"@kittr/types": "file:packages/types",
		"@sentry/node": "^6.18.0",
		"axios": "^0.26.0",
		"cors": "2.8.5",
		"cron": "1.8.2",
		"dotenv": "16.0.0",
		"express": "^4.17.3",
		"jsonwebtoken": "8.5.1",
		"module-alias": "2.2.2",
		"mongoose": "^6.2.3",
		"nodemon": "^2.0.15",
		"socket.io": "4.4.1"
	},
	"devDependencies": {
		"@types/cron": "1.7.3",
		"@types/express": "4.17.13",
		"@types/jsonwebtoken": "8.5.8",
		"@types/module-alias": "2.0.1",
		"@types/node": "^17.0.21",
		"@typescript-eslint/eslint-plugin": "^5.12.0",
		"@typescript-eslint/parser": "^5.12.0",
		"tsconfig-paths": "^3.12.0",
		"typescript": "4.5.5"
	}
}
