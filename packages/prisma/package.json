{
	"name": "@kittr/prisma",
	"version": "0.1.1",
	"private": true,
	"scripts": {
		"clean": "rm -rf .turbo && rm -rf node_modules",
		"db-deploy": "prisma migrate deploy",
		"db-migrate": "prisma db push --force-reset",
		"generate-schemas": "prisma generate",
		"db-studio": "prisma studio",
		"pls": "prisma db push --force-reset && ts-node ./migration-scripts/index.ts",
		"nuke": "ts-node ./migration-scripts/nuke.ts",
		"format": "prisma format"
	},
	"devDependencies": {
		"prisma": "3.9.2",
		"ts-node": "10.5.0",
		"typescript": "4.5.5"
	},
	"dependencies": {
		"@prisma/client": "3.9.2",
		"mongoose": "6.2.3"
	},
	"main": "index.ts",
	"types": "index.d.ts",
	"prisma": {
		"seed": "ts-node ./seed.ts"
	}
}
