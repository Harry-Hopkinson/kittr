{
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", "build/**"]
		},
		"test": {
			"dependsOn": ["^build"],
			"outputs": ["coverage/**"]
		},

		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false
		},
		"clean": {
			"cache": false
		},
		"type-check": {
			"outputs": []
		}
	}
}
